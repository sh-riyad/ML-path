  <brief_title>Improved Oxygen Therapy in Covid-19</brief_title>
  <official_title>A Mask on Top of HFNC May Improve Oxygenation in Covid19</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Region Skane</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
