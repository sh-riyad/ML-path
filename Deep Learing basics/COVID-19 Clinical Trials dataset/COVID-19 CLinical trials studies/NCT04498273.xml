  <eligibility>
    <criteria>
      <textblock>
        Inclusion:&#xD;
&#xD;
          -  COVID-19+ in past 14 days&#xD;
&#xD;
          -  Platelets &gt; 100,000&#xD;
&#xD;
          -  eGFR &gt; 30ml/min&#xD;
&#xD;
        Exclusion:&#xD;
&#xD;
          -  Hospitalized&#xD;
&#xD;
          -  Contradiction/ other indication for anti-coagulation&#xD;
&#xD;
          -  Pregnancy&#xD;
&#xD;
          -  Active cancer&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>40 Years</minimum_age>
    <maximum_age>80 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
